    "# Ask the user for their age\n",
    "age = int(input(\"Please enter your age: \"))\n",
    "\n",
    "# Check if the user is at least 18 years old\n",
    "if age >= 18:\n",
    "    print(\"You are eligible to vote.\")\n",
    "else:\n",
    "    print(\"You are not eligible to vote yet.\")"
